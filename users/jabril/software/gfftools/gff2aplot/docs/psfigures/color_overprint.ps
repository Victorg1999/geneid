%
/colorary [ (red) % RGB
            (darkgreen)
            (blue)
            (verylightcyan) % CMYK
            (darkmagenta)
            (yellow)
            (black)
          ] D
%
/tnm  7 D % track number
/twd 15 D % track width
/twh twd 2 div D % track width
/tws twd twh add D
/TWD twd tnm mul twh tnm 1 add mul add D % sub-figure width
%
/drwcolbox { % x-center y-bottom lbl drwcolbox
  S
    /lbl X
    T % goto X Y
	S
	  TWD 2 div twd neg T
	  twd /Helvetica-Bold sfnt
      lbl stringwidth pop 2 div neg -5 m
	    lbl show
	R
    0 twh % put counter and first coord on stack
    tnm {
      2 copy
	  /x X
	  /c X
	  colorary c get dup % get color name
	  S
	    x 0 x twd add TWD bbox
	    cvx exec % get color
	    S cmyk fill R black cmyk K
      R
	  S
		0 x TWD x twd add bbox
	    cvx exec % get color
		S cmyk fill R black cmyk K
	  R
	  tws add exch % increase coord
	  1 add exch % increase counter
    } repeat
    pop pop % remove counter and coord from stack
  R
} B % drwcolbox
