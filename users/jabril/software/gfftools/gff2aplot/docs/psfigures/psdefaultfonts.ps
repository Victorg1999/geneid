%%BeginProlog
%
%%BeginProcSet: Short_Names 1.0 0
%
/B { bind def } bind def
/X { exch def } B
/x { exch } B
/r { roll } B
/D { def } B
/S { gsave } B
/R { grestore } B
/F { scale } B
/T { translate } B
/A { rotate } B
/m { moveto } B
/rm { rmoveto } B
/l { lineto } B
/rl { rlineto } B
/k { stroke } B
/gk { S stroke R } B
/f { fill } B
/gf { S fill R } B
/ie { ifelse } B
/cmyk { setcmykcolor } B
/slw { setlinewidth } B
/sfnt { findfont x scalefont setfont } B
%
%%EndProcSet: Short_Names 1.0 0
%
%%BeginProcSet: Colors 1.0 0
%
/black { 0.00 0.00 0.00 1.00 } D /white { 0.00 0.00 0.00 0.00 } D
%
%%EndProcSet: Colors 1.0 0
%
%%BeginProcSet: Functions 1.0 0
%
% /pgA4 { 595 842 } D
/cm { 28.35 mul } B
/in { 72 mul } B
/bbox { 4 copy 3 1 r x 6 2 r 8 -2 r m l l l closepath } B
%
%%EndProcSet: Functions 1.0 0
%
%%BeginProcSet: Local_Functions 1.0 0
%
/fsz 10 D /ifsz fsz 1.35 mul D /spc 5 D
/oY 800 D /oX 180 D
/nY { /oY oY ifsz 3.45 mul sub D oY } B
/sY { /oY oY ifsz sub D } B
/strU (ABCDEFGHIJKLMNOPQRSTUVWXYZ) D
/strL (abcdefghijklmnopqrstuvwxyz) D
/strN (1234567890 @#$%&_-+*?!\\/|[]{}<>\(\)) D
/ex { S oX x T fsz x sfnt spc 0 m strU show spc ifsz neg m strL show spc ifsz 2 mul neg m strN show fsz /Courier sfnt dup stringwidth pop spc add neg 0 m show R } B
%
%%EndProcSet: Local_Functions 1.0 0
%
%%EndProlog
%
%%BeginSetup
%
% initgraphics % must not be used in EPS documents
true setstrokeadjust
0.125 setlinewidth
0 setlinejoin
0 setlinecap
%
%%EndSetup
%
%%Page: 1 1
%%BeginPageSetup
%
/pgsave save def
%
%%EndPageSetup
%
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
%
black cmyk
% serif
(Times-Roman)           /Times-Roman           oY ex
(Times-Italic)          /Times-Italic          nY ex
(Times-Bold)            /Times-Bold            nY ex
(Times-BoldItalic)      /Times-BoldItalic      nY ex
sY
% sans serif
(Helvetica)             /Helvetica             nY ex
(Helvetica-Oblique)     /Helvetica-Oblique     nY ex
(Helvetica-Bold)        /Helvetica-Bold        nY ex
(Helvetica-BoldOblique) /Helvetica-BoldOblique nY ex
sY
% monospaced
(Courier)               /Courier               nY ex
(Courier-Oblique)       /Courier-Oblique       nY ex
(Courier-Bold)          /Courier-Bold          nY ex
(Courier-BoldOblique)   /Courier-BoldOblique   nY ex
%
% plot title
nY pop nY
oX x m fsz /Courier-Bold sfnt (<<psdefaultfonts.ps (font size = 10pt)>>) show
%
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
%
pgsave restore
showpage
%
