/solid { [ ] 0 setdash } D
/dotted { [ 1 ] 0 setdash } D
/ldotted { [ 1 2 ] 0 setdash } D
/dashed { [ 3 3 ] 0 setdash } D
/ldashed { [ 6 4 ] 0 setdash } D
/ddashed { [ 4 3 1 3 ] 0 setdash } D
%
/sfnt { findfont exch scalefont setfont } B
/glblw { sfnt (X) stringwidth pop mul } B
/chrh { S newpath 0 0 m _f charpath flattenpath pathbbox exch pop 3 -1 roll pop R } B
/strh { 2 dict begin /lly 0.0 D /ury 0.0 D { ( ) dup 0 4 -1 roll put chrh dup ury gt { /ury X } { pop } ie dup lly lt { /lly X } { pop } ie } forall ury end } B
/strlly { 2 dict begin /lly 0.0 D /ury 0.0 D { ( ) dup 0 4 -1 roll put chrh dup ury gt { /ury X } { pop } ie dup lly lt { /lly X } { pop } ie } forall lly end } B
/ttshw { S cmyk sfnt 8 dict begin /h X /v X /lbl X /angle X /y X /x X /hs lbl stringwidth pop D /vs lbl strh D x y T angle rotate angle 90 gt { -1 -1 F } if h (rh) eq { hs } { h (ch) eq { hs 2 div } { 0 } ie } ie neg v (tv) eq { vs } { v (cv) eq { vs 2 div } { 0 } ie } ie neg m lbl show end R } D
%
/w 100 D
/hw w 2 div D
/Xo 40 D
/Yo 700 D
/Xc Xo D
/Yc Yo D
/Xs w 1.35 mul D
/Ys hw 1.5 mul D
/Xn { /Xc Xc Xs add D Xc } D
/Yn { /Yc Yc Ys sub D Yc } D
/Xr { /Xc Xo D Xc } D
/Yr { /Yc Yo D Yc } D
/s (text string) D
/th 10 D
/tf { th /Helvetica-Bold } D
/tw S tf sfnt s stringwidth pop R D
/htw tw 2 div D
%
/bline { S hw neg 0 m hw 0 l 0 0 m 0 hw l 1 slw solid darkgrey cmyk k R } B
/aline { S 0 0 m 0 th l 0 0 m htw 0 l 0.5 slw dotted red cmyk k R } B
/getXp {
	dup sin 0 eq {
	    pop (ch)
	} {
		90 sub cos 0 ge {
			(lh)
		} {
			(rh)
		} ie
	} ie
} B
/getYp {
	dup cos 0 eq {
		pop (cv)
	} {
		90 sub sin 0 ge {
			(tv)
		} {	
			(bv)
		} ie
	} ie
} B
/bpoint {
 S
  black cmyk
  tf sfnt
  0 0 T
  % cmyk sfnt
  9 dict begin
    % /h X /v X /lbl X
    /angle X
    /h angle getXp D
    /v angle getYp D
	/lbl s D
    % /y X /x X
    /hs lbl stringwidth pop D /vs lbl strh D
    /vfx lbl strlly D
    % x y T
	angle rotate
    % angle 90 gt { -1 -1 F } if
    h (rh) eq { hs } { h (ch) eq { hs 2 div } { 0 } ie } ie neg
    v (tv) eq { vs } { v (cv) eq { vs 2 div } { 0 } ie } ie neg
    S 0 0 2.5 0 360 arc skyblue cmyk gf 0.25 slw black cmyk k R
    T
    S 0 vfx T 0 0 tw th bbox lightgreen cmyk 0.5 slw dotted k R
    S 0 0 m tw 0 l red cmyk 0.25 slw solid k R
    S 0 0 1.5 0 360 arc red cmyk gf 0.25 slw black cmyk k R
    0 0 m lbl show
  end
 R
} B
/fs 10 D % /fs 8 D
/title {
 S
  { -90 A } if hw neg hw T
  % dup dup getYp 3 1 r getXp x
  3 string cvs
  fs /Helvetica-Bold sfnt
  0 fs neg m show
  % 0 fs neg 2.05 mul m show
  % 0 fs neg 3.10 mul m show
 R
} B
%
/simpletext { S 0 0 m tf sfnt s show R } B
/ex { S x hw add x T
        bline dup bpoint _f title R } B % simpletext R } B
/ey { S x hw add x hw add T
        90 A
        bline dup bpoint _t title R } B % simpletext R } B
