# Elements taken from GFF_random_sample.txt, generated with following script:
#
# Random positions generation: 
#   gawk 'BEGIN{source[1]="Experimental";source[2]="DB-Match";source[3]="Feature_Finder";source[4]="Genomic_Explorer";source[5]="HMM+";source[6]="PoSsearch";for (k=1;k<=1000;k++) {n=randint(1,10000);m=randint(n+100,n+1000);printf "Example\t%s\t%s\t%s\t%s\t\n",source[randint(1,6)],n,m,randint(0,100)}} function randint(i,e){return int((e-i)*rand())+i}' | sort -b +1 -2 +2n > GFF_random_sample.txt
#
# Calculating correct frames:
#   gawk '{rmd=(3-($2-($1+$3)+1) % 3) % 3;printf "%s %s %s :: %s \n",$1,$2,$3,rmd}'
#   gawk '{if ($1=="Example" && $3=="exon") { if (prev==0) {print $0"\t0 ("(3-($5-$4+1) % 3) % 3 ")*f";prev=1;lstfrm=0} else {print $0"\t"rmd,"("(3-($5-($4+rmd)+1) % 3) % 3")"}} else {print $0; prev=0}; rmd=(3-($5-($4+lstfrm)+1) % 3) % 3; lstfrm=rmd }' GFF_sample.gff  ( | mpage -1 -l -L50 | lpr )
