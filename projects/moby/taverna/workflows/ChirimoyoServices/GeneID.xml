<?xml version="1.0" encoding="UTF-8"?>
<s:scufl xmlns:s="http://org.embl.ebi.escience/xscufl/0.1alpha" version="0.2" log="0">
  <s:workflowdescription lsid="urn:lsid:www.mygrid.org.uk:operation:4EN3STOHL67" author="" title="" />
  <s:processor name="Create_moby_data">
    <s:local>org.embl.ebi.escience.scuflworkers.biomoby.CreateMobyData</s:local>
  </s:processor>
  <s:processor name="parse_moby_sequence_data">
    <s:local>org.embl.ebi.escience.scuflworkers.biomoby.ExtractMobyData</s:local>
  </s:processor>
  <s:processor name="parse_moby_GFF_data">
    <s:local>org.embl.ebi.escience.scuflworkers.biomoby.ExtractMobyData</s:local>
  </s:processor>
  <s:processor name="getNucleotideSeqfromEMBL">
    <s:description>Retrieves a nucleotide sequence given a EMBL Id or Accession Number</s:description>
    <s:biomobywsdl>
      <s:mobyEndpoint>http://www.inab.org/cgi-bin/MOBY-Central.pl</s:mobyEndpoint>
      <s:serviceName>getNucleotideSeqfromEMBL</s:serviceName>
      <s:authorityName>pdg.cnb.uam.es</s:authorityName>
    </s:biomobywsdl>
  </s:processor>
  <s:processor name="runGeneIDGFF">
    <s:description>Ab initio gene prediction tool - Return the output predictions in GFF format, See http://www.sanger.ac.uk/Software/formats/GFF</s:description>
    <s:biomobywsdl>
      <s:mobyEndpoint>http://chirimoyo.ac.uma.es/cgi-bin/MOBY-Central.pl</s:mobyEndpoint>
      <s:serviceName>runGeneIDGFF</s:serviceName>
      <s:authorityName>genome.imim.es</s:authorityName>
    </s:biomobywsdl>
  </s:processor>
  <s:processor name="GenericSequence2FASTA">
    <s:description>Converson of MOBY Generic Sequence objects to FASTA objects</s:description>
    <s:biomobywsdl>
      <s:mobyEndpoint>http://mobycentral.icapture.ubc.ca/cgi-bin/MOBY05/mobycentral.pl</s:mobyEndpoint>
      <s:serviceName>GenericSequence2FASTA</s:serviceName>
      <s:authorityName>icapture.ubc.ca</s:authorityName>
    </s:biomobywsdl>
  </s:processor>
  <s:link source="sequenceID" sink="Create_moby_data:id" />
  <s:link source="namespace" sink="Create_moby_data:namespace" />
  <s:link source="Create_moby_data:mobydata" sink="getNucleotideSeqfromEMBL:input" />
  <s:link source="getNucleotideSeqfromEMBL:output" sink="runGeneIDGFF:input" />
  <s:link source="parse_moby_sequence_data:value" sink="sequence" />
  <s:link source="runGeneIDGFF:output" sink="parse_moby_GFF_data:mobydata" />
  <s:link source="parse_moby_GFF_data:value" sink="GeneID Predictions" />
  <s:link source="getNucleotideSeqfromEMBL:output" sink="Sequence_MOBY" />
  <s:link source="getNucleotideSeqfromEMBL:output" sink="GenericSequence2FASTA:input" />
  <s:link source="GenericSequence2FASTA:output" sink="parse_moby_sequence_data:mobydata" />
  <s:source name="sequenceID" />
  <s:source name="namespace" />
  <s:sink name="sequence">
    <s:metadata>
      <s:mimeTypes>
        <s:mimeType>text/plain</s:mimeType>
        <s:mimeType>text/xml</s:mimeType>
      </s:mimeTypes>
    </s:metadata>
  </s:sink>
  <s:sink name="GeneID Predictions">
    <s:metadata>
      <s:mimeTypes>
        <s:mimeType>text/xml</s:mimeType>
        <s:mimeType>text/plain</s:mimeType>
      </s:mimeTypes>
    </s:metadata>
  </s:sink>
  <s:sink name="Sequence_MOBY">
    <s:metadata>
      <s:mimeTypes>
        <s:mimeType>text/xml</s:mimeType>
      </s:mimeTypes>
    </s:metadata>
  </s:sink>
</s:scufl>


