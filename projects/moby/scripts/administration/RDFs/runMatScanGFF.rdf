<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF
    xmlns:serviceInstances="http://biomoby.org/RESOURCES/MOBY-S/ServiceInstances#"
    xmlns:mobyServiceDescription="http://biomoby.org/RESOURCES/MOBY-S/ServiceDescription#"
    xmlns:mygrid="http://www.mygrid.org.uk/ontology#"
    xmlns:mobyObject="http://biomoby.org/RESOURCES/MOBY-S/Objects#"
    xmlns:mobyNamespace="http://biomoby.org/RESOURCES/MOBY-S/Namespaces#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:mobyService="http://biomoby.org/RESOURCES/MOBY-S/Services#"
    xmlns:dc="http://purl.org/dc/elements/1.1/">
     <mygrid:serviceDescription rdf:about="urn:lsid:biomoby.org:serviceinstance:genome.imim.es,runMatScanGFF">
          <mygrid:locationURI>http://genome.imim.es/cgi-bin/moby/MobyServices.cgi</mygrid:locationURI>
          <mygrid:hasServiceDescriptionText>Promoter regions analysis program. Matscan is a program to search putative binding sites in genomic sequences. You can search for example the Transfac database, but also MEME or jaspar matrices. This service only accepts a simple DNASequence object and returns a simple GFF object. If you want to give MatScan output to Meta-alignment program, you MUST use the 'log-likelihood matrix' mode.</mygrid:hasServiceDescriptionText>
          <mygrid:providedBy>
               <mygrid:organisation>
                    <mobyServiceDescription:authoritative>false</mobyServiceDescription:authoritative>
                    <dc:publisher>genome.imim.es</dc:publisher>
                    <dc:creator>akerhornou@imim.es</dc:creator>
               </mygrid:organisation>
          </mygrid:providedBy>
          <mygrid:hasServiceNameText>runMatScanGFF</mygrid:hasServiceNameText>
          <mygrid:hasServiceDescriptionLocation></mygrid:hasServiceDescriptionLocation>
          <mygrid:hasOperation>
               <mygrid:operation>
                    <mygrid:inputParameter>
                         <mygrid:parameter>
                              <mygrid:hasParameterType>
                                   <mygrid:simpleParameter/>
                              </mygrid:hasParameterType>
                              <mobyServiceDescription:objectType rdf:resource="http://biomoby.org/RESOURCES/MOBY-S/Objects#DNASequence"/>
                              <mygrid:hasParameterNameText>upstream_sequences</mygrid:hasParameterNameText>
                         </mygrid:parameter>
                    </mygrid:inputParameter>
                    <mygrid:performsTask>
                         <mobyService:Analysis>
                              <rdf:type rdf:resource="http://www.mygrid.org.uk/ontology#operationTask"/>
                         </mobyService:Analysis>
                    </mygrid:performsTask>
                    <mygrid:inputParameter>
                         <mygrid:parameter>
                              <mygrid:hasParameterNameText rdf:datatype=
                              "http://www.w3.org/2001/XMLSchema#string"
                              >threshold</mygrid:hasParameterNameText>
                              <mobyServiceDescription:datatype
                               rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                              >Float</mobyServiceDescription:datatype>
                              <mygrid:hasDefaultValue rdf:datatype=
                              "http://www.w3.org/2001/XMLSchema#string"
                              >0.85</mygrid:hasDefaultValue>
                              <mygrid:hasParameterType>
                                   <mygrid:secondaryParameter/>
                              </mygrid:hasParameterType>
                         </mygrid:parameter>
                    </mygrid:inputParameter>
                    <mygrid:inputParameter>
                         <mygrid:parameter>
                              <mygrid:hasParameterNameText rdf:datatype=
                              "http://www.w3.org/2001/XMLSchema#string"
                              >matrix mode</mygrid:hasParameterNameText>
                              <mobyServiceDescription:enum rdf:datatype=
                              "http://www.w3.org/2001/XMLSchema#string"
                              >raw format</mobyServiceDescription:enum>
                              <mygrid:hasDefaultValue rdf:datatype=
                              "http://www.w3.org/2001/XMLSchema#string"
                              >log-likelihood</mygrid:hasDefaultValue>
                              <mygrid:hasParameterType>
                                   <mygrid:secondaryParameter/>
                              </mygrid:hasParameterType>
                              <mobyServiceDescription:datatype
                               rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                              >String</mobyServiceDescription:datatype>
                              <mobyServiceDescription:enum rdf:datatype=
                              "http://www.w3.org/2001/XMLSchema#string"
                              >log-likelihood</mobyServiceDescription:enum>
                         </mygrid:parameter>
                    </mygrid:inputParameter>
                    <mygrid:inputParameter>
                         <mygrid:parameter>
                              <mygrid:hasParameterNameText rdf:datatype=
                              "http://www.w3.org/2001/XMLSchema#string"
                              >matrix</mygrid:hasParameterNameText>
                              <mobyServiceDescription:enum rdf:datatype=
                              "http://www.w3.org/2001/XMLSchema#string"
                              >MEME</mobyServiceDescription:enum>
                              <mygrid:hasParameterType>
                                   <mygrid:secondaryParameter/>
                              </mygrid:hasParameterType>
                              <mobyServiceDescription:enum rdf:datatype=
                              "http://www.w3.org/2001/XMLSchema#string"
                              >Transfac</mobyServiceDescription:enum>
                              <mygrid:hasDefaultValue rdf:datatype=
                              "http://www.w3.org/2001/XMLSchema#string"
                              >Transfac</mygrid:hasDefaultValue>
                              <mobyServiceDescription:enum rdf:datatype=
                              "http://www.w3.org/2001/XMLSchema#string"
                              >Jaspar</mobyServiceDescription:enum>
                              <mobyServiceDescription:datatype
                               rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                              >String</mobyServiceDescription:datatype>
                         </mygrid:parameter>
                    </mygrid:inputParameter>
                    <mygrid:inputParameter>
                         <mygrid:parameter>
                              <mobyServiceDescription:enum rdf:datatype=
                              "http://www.w3.org/2001/XMLSchema#string"
                              >Reverse</mobyServiceDescription:enum>
                              <mygrid:hasParameterType>
                                   <mygrid:secondaryParameter/>
                              </mygrid:hasParameterType>
                              <mobyServiceDescription:enum rdf:datatype=
                              "http://www.w3.org/2001/XMLSchema#string"
                              >Both</mobyServiceDescription:enum>
                              <mobyServiceDescription:datatype
                               rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                              >String</mobyServiceDescription:datatype>
                              <mobyServiceDescription:enum rdf:datatype=
                              "http://www.w3.org/2001/XMLSchema#string"
                              >Forward</mobyServiceDescription:enum>
                              <mygrid:hasDefaultValue rdf:datatype=
                              "http://www.w3.org/2001/XMLSchema#string"
                              >Both</mygrid:hasDefaultValue>
                              <mygrid:hasParameterNameText rdf:datatype=
                              "http://www.w3.org/2001/XMLSchema#string"
                              >strands</mygrid:hasParameterNameText>
                         </mygrid:parameter>
                    </mygrid:inputParameter>
                    <mygrid:outputParameter>
                         <mygrid:parameter>
                              <mygrid:hasParameterNameText>matscan_predictions</mygrid:hasParameterNameText>
                              <mobyServiceDescription:objectType rdf:resource="http://biomoby.org/RESOURCES/MOBY-S/Objects#GFF"/>
                              <mygrid:hasParameterType>
                                   <mygrid:simpleParameter/>
                              </mygrid:hasParameterType>
                         </mygrid:parameter>
                    </mygrid:outputParameter>
                    <mygrid:hasOperationNameText>runMatScanGFF</mygrid:hasOperationNameText>
               </mygrid:operation>
          </mygrid:hasOperation>
          <dc:format>moby</dc:format>
     </mygrid:serviceDescription>
</rdf:RDF>
