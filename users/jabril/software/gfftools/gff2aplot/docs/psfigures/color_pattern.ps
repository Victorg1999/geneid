/colorary [ (red) % RGB
            (darkgreen)
            (blue)
            (verylightcyan) % CMYK
            (darkmagenta)
            (yellow)
            (black)
          ] D
%
/tnm  7 D % track number
/twd 15 D % track width
/twh twd 2 div D % track width
/tws twd twh add D
/TWD twd tnm mul twh tnm 1 add mul add D % sub-figure width
%
/alldata { % x-center y-bottom dot_pitch alldata 
 /mybox save D
  /AntiAliasColorImages false D
  /AntiAliasGrayImages false D
  /AntiAliasMonoImages false D
  /PG X
  /PY X
  /PX X
  /PS PG 2 mul D
  %
  << %% Begin prototype pattern dictionary
    /PaintType   2 %% Required: 2 - uncolored pattern
    /PatternType 1 %% Required: must be "1"
    /TilingType  1 %% Required: 1 - constant spacing
    /BBox [ PG neg PG neg  PG PG ] %% Required: pattern cell bounding box
    /XStep PS %% Required: horizontal spacing between pattern cells
    /YStep PS %% Required: vertical spacing between pattern cells
    /PaintProc { %% Required: PS procedure for painting the pattern cell
      pop %% pop pattern dictionary
      % 0 setlinewidth 0 setlinecap
      % 0 0 PG     PG     bbox S fill R K
      % 0 0 PG neg PG neg bbox S fill R K 
      PG setlinewidth 0 setlinecap
      PG 2 div     0 moveto 0 PG     rlineto stroke
      PG 2 div neg 0 moveto 0 PG neg rlineto stroke	  
    }
  >> %% End prototype pattern dictionary
  matrix % identity matrix
  makepattern % instantiate the pattern
  /HP X
  %
  << %% Begin prototype pattern dictionary
    /PaintType   2 %% Required: 2 - uncolored pattern
    /PatternType 1 %% Required: must be "1"
    /TilingType  1 %% Required: 1 - constant spacing
    /BBox [ PG neg PG neg  PG PG ] %% Required: pattern cell bounding box
    /XStep PS %% Required: horizontal spacing between pattern cells
    /YStep PS %% Required: vertical spacing between pattern cells
    /PaintProc { %% Required: PS procedure for painting the pattern cell
      pop %% pop pattern dictionary
%	  0 setlinewidth 0 setlinecap
%	  0 0 PG neg PG     bbox fill
%	  0 0 PG     PG neg bbox fill
      PG setlinewidth 0 setlinecap
      PG 2 div neg 0 moveto 0 PG     rlineto stroke
      PG 2 div     0 moveto 0 PG neg rlineto stroke	  
    }
  >> %% End prototype pattern dictionary
  matrix % identity matrix
  makepattern % instantiate the pattern
  /VP X
  %
  PX PY PG drwcolbox
 mybox restore
} B % alldata
%
/drwcolbox {
  S
    /pg X
    T % goto X Y
	S
	  TWD 2 div 10 twh add sub twd neg T
      -10 -10 10 10 bbox S 1 slw black cmyk K R white cmyk fill newpath
      red cmyk 10 slw 0 setlinecap
      S -5 0 moveto 0  10 rlineto stroke R
      S  5 0 moveto 0 -10 rlineto stroke R
      twd -5 m % 90 rotate
	    twd /Helvetica-Bold sfnt
	    pg 6 string cvs show ( pt) show
	R
    [ /Pattern /DeviceCMYK ] setcolorspace % pattern-filling CMYK colors
    0 twh % put counter and first coord on stack
    tnm {
      2 copy
	  /x X
	  /c X
	  colorary c get dup % get color name
	  S
	    x 0 x twd add TWD bbox
  	    % S c 10 string cvs print (\n) print flush R
	    cvx exec % get color
		% S 4 copy mark 5 -4 roll counttomark dup { dup index 20 string cvs print ( ) print 1 sub } repeat (\n) print 6 { pop } repeat flush R
	    S HP setcolor fill R black cmyk K
      R
	  S
		0 x TWD x twd add bbox
  	    % S dup 20 string cvs print (\n) print flush R
	    cvx exec % get color
		% S 4 copy mark 5 -4 roll counttomark dup { dup index 20 string cvs print ( ) print 1 sub } repeat (\n) print 6 { pop } repeat flush R
		S VP setcolor fill R black cmyk K
	  R
	  tws add exch % increase coord
	  1 add exch % increase counter
    } repeat
    pop pop % remove counter and coord from stack
  R
} B % drwcolbox
