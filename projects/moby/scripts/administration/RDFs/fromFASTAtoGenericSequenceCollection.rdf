<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF
    xmlns:serviceInstances="http://biomoby.org/RESOURCES/MOBY-S/ServiceInstances#"
    xmlns:mobyServiceDescription="http://biomoby.org/RESOURCES/MOBY-S/ServiceDescription#"
    xmlns:mygrid="http://www.mygrid.org.uk/ontology#"
    xmlns:mobyObject="http://biomoby.org/RESOURCES/MOBY-S/Objects#"
    xmlns:mobyNamespace="http://biomoby.org/RESOURCES/MOBY-S/Namespaces#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:mobyService="http://biomoby.org/RESOURCES/MOBY-S/Services#"
    xmlns:dc="http://purl.org/dc/elements/1.1/">
     <mygrid:serviceDescription rdf:about="urn:lsid:biomoby.org:serviceinstance:genome.imim.es,fromFASTAtoGenericSequenceCollection">
          <mygrid:providedBy>
               <mygrid:organisation>
                    <mobyServiceDescription:authoritative>false</mobyServiceDescription:authoritative>
                    <dc:creator>akerhornou@imim.es</dc:creator>
                    <dc:publisher>genome.imim.es</dc:publisher>
               </mygrid:organisation>
          </mygrid:providedBy>
          <mygrid:locationURI>http://genome.imim.es/cgi-bin/moby/MobyServices.cgi</mygrid:locationURI>
          <dc:format>moby</dc:format>
          <mygrid:hasServiceDescriptionText>Converts a FASTA object into a collection of GenericSequence moby objects</mygrid:hasServiceDescriptionText>
          <mygrid:hasServiceDescriptionLocation></mygrid:hasServiceDescriptionLocation>
          <mygrid:hasOperation>
               <mygrid:operation>
                    <mygrid:outputParameter>
                         <mygrid:parameter>
                              <mygrid:hasParameterNameText>sequences</mygrid:hasParameterNameText>
                              <mobyServiceDescription:objectType rdf:resource="http://biomoby.org/RESOURCES/MOBY-S/Objects#GenericSequence"/>
                              <mygrid:hasParameterType>
                                   <mygrid:collectionParameter/>
                              </mygrid:hasParameterType>
                         </mygrid:parameter>
                    </mygrid:outputParameter>
                    <mygrid:performsTask>
                         <mygrid:operationTask>
                              <rdf:type rdf:resource="http://biomoby.org/RESOURCES/MOBY-S/Services#Conversion"/>
                         </mygrid:operationTask>
                    </mygrid:performsTask>
                    <mygrid:hasOperationNameText>fromFASTAtoGenericSequenceCollection</mygrid:hasOperationNameText>
                    <mygrid:inputParameter>
                         <mygrid:parameter>
                              <mobyServiceDescription:objectType rdf:resource="http://biomoby.org/RESOURCES/MOBY-S/Objects#FASTA"/>
                              <mygrid:hasParameterType>
                                   <mygrid:simpleParameter/>
                              </mygrid:hasParameterType>
                              <mygrid:hasParameterNameText>sequences</mygrid:hasParameterNameText>
                         </mygrid:parameter>
                    </mygrid:inputParameter>
               </mygrid:operation>
          </mygrid:hasOperation>
          <mygrid:hasServiceNameText>fromFASTAtoGenericSequenceCollection</mygrid:hasServiceNameText>
     </mygrid:serviceDescription>
